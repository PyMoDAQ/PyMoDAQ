USB\VID_0A2D&PID_001B.DeviceDesc="7270 DSP Lock-in Amplifier"
USB\VID_0A2D&PID_0027.DeviceDesc="7230 DSP Lock-in Amplifier"
DeviceClassString="NI-VISA USB Devices"
WinUsb_Service_DisplayName="WinUSB Driver"

