[Version]
Signature=$WINDOWS NT$
Class=visaUsbDevice
ClassGUID={A3330EDF-239D-4206-833B-1D58952613D5}
Provider=%Vendor0%
DriverVer=02/13/2010,2.0
CatalogFile=SRUSBVISA_W7Vista.cat

;===========================================================================
;  Default Installer
;===========================================================================

[DefaultInstall]
CopyINF=SRUSBVISA_W7Vista.inf

[DestinationDirs]

[SourceDisksNames]

[SourceDisksFiles]

;===========================================================================
;  Class Installer
;===========================================================================

[ClassInstall32]
AddReg=AddClass_AddReg

[AddClass_AddReg]
HKR,,,0,%DeviceClassString%
HKR,,Icon,,"-20"

;===========================================================================

[Manufacturer]
%Vendor1%=USBList,NTamd64

[USBList]
%USB\VID_0A2D&PID_0018.DeviceDesc%=WinUsb_Inst, USB\VID_0A2D&PID_0018
%USB\VID_0A2D&PID_001B.DeviceDesc%=WinUsb_Inst, USB\VID_0A2D&PID_001B
%USB\VID_0A2D&PID_0027.DeviceDesc%=WinUsb_Inst, USB\VID_0A2D&PID_0027

